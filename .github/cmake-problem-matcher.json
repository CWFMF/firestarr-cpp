{
  "problemMatcher": [
    {
      "owner": "cmake",
      "pattern": [
        {
          "regexp": "^(.+):(.+):(.+): (error|warning): (.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "cmake-windows",
      "pattern": [
        {
          "regexp": "^(.+)\\((\\d+)\\): (error|warning) (.+): (.+)$",
          "file": 1,
          "line": 2,
          "severity": 3,
          "code": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "cmake-windows-external",
      "pattern": [
        {
          "regexp": "^(.*\\\\MSVC\\\\.+\\\\.+\\(\\d+\\): (error|warning) (.+): .+)$",
          "severity": 2,
          "code": 3,
          "message": 1
        }
      ]
    },
    {
      "owner": "cmake-other",
      "pattern": [
        {
          "regexp": "^([^\\(]+ *: (warning|error): .+)$",
          "severity": 2,
          "message": 1
        }
      ]
    },
    {
      "owner": "cmake-windows-other",
      "pattern": [
        {
          "regexp": "^([^\\(]+ *: Command line (warning|error) (.+) : .+)$",
          "severity": 2,
          "code": 3,
          "message": 1
        }
      ]
    }
  ]
}